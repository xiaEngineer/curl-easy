{
    "name": "xiaengine/curl-easy",
    "type": "library",
    "description": "cURL wrapper for PHP. Supports parallel and non-blocking requests. For high speed crawling, see stil/curl-robot.",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "cURL\\": "src/"
        },
        "psr-0": {
            "cURL": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cURL\\Tests\\": "tests/"
        }
    }
}